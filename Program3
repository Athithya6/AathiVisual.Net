using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication3
{
    class Program
    {
        static void Main(string[] args)
        {
            string str;
            Console.WriteLine("Enter a string:");
            str = Console.ReadLine();
            foreach(char c in str)
            {
                Console.WriteLine(c);
            }
        }
    }
}
