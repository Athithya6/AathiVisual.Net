using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication5
{
    class Program
    {
        static string whereAmI;
        static void Main(string[] args)
        {
            f1();
            f2();
        }
        static void f1()
        {
            whereAmI = "I am in function-I";
        }
        static void f2()
        {
            whereAmI = "I am in function-II";
        }
    }
}
